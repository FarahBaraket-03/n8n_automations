      "typeVersion": 3.2
    },
    {
      "parameters": {
        "fields": {
          "values": [
            {
              "name": "query.maxlimit",
              "type": "numberValue",
              "numberValue": "={{ $json?.query?.maxlimit == null ? 70000 : Number($json?.query?.maxlimit) }}"
            },
            {
              "name": "query.url",
              "stringValue": "={{ $json.query.replace(/[),]+$/g, \"\").trim() }}"
            }
          ]
        },
        "options": {}
      },
      "id": "c06a95ad-acba-40f8-8af0-f7e29962ac34",
