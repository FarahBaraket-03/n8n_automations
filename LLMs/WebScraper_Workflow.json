      ],
      "typeVersion": 1
    },
    {
      "parameters": {
        "url": "={{ encodeURI($json.query.url) }}",
        "options": {
          "allowUnauthorizedCerts": true,
          "response": {
            "response": {
              "neverError": true
            }
          }
        }
      },
      "id": "f129185b-79f9-4346-b819-0e71299e0170",
      "name": "HTTP Request",
      "type": "n8n-nodes-base.httpRequest",
      "position": [
        624,
        672
      ],
      "typeVersion": 4.1,
      "alwaysOutputData": false,
      "onError": "continueRegularOutput"
    },
    {
      "parameters": {
        "fields": {
          "values": [
            {
              "name": "HTML",
              "stringValue": "={{ $json?.data.match(/<body[^>]*>([\\s\\S]*?)<\\/body>/i)[1] }}"
            }
          ]
        },
        "include": "selected",
        "includeFields": "HTML",
        "options": {}
      },
      "id": "a5a2d68e-ac99-408a-a543-3f77fa8e71a5",
      "name": "Exctract HTML Body",
      "type": "n8n-nodes-base.set",
      "position": [
        1056,
        944
      ],
      "typeVersion": 3.2
    },
    {
      "parameters": {
        "conditions": {
          "boolean": [
            {
              "value1": "={{ $json.hasOwnProperty('error') }}",
              "value2": true
            }
          ]
        }
      },
      "id": "e10eb2b7-aa4e-4527-8b7c-24cc6de36d92",
      "name": "Is error?",
      "type": "n8n-nodes-base.if",
      "position": [
        832,
        672
      ],
      "typeVersion": 1
    },
    {
      "parameters": {
        "fields": {
          "values": [
            {
              "name": "page_content",
              "stringValue": "={{ $('QUERY_PARAMS').first()?.json?.query?.url == null ? \"INVALID action_input. This should be an HTTP query string like this: \\\"?url=VALIDURL&method=SELECTEDMETHOD\\\". Only a simple string value is accepted. JSON object as an action_input is NOT supported!\" : JSON.stringify($json.error) }}"
            }
          ]
        },
        "include": "selected",
        "includeFields": "HTML",
        "options": {}
      },
      "id": "6ab90f6d-5826-49a3-be00-a7130eef8094",
      "name": "Stringify error message",
      "type": "n8n-nodes-base.set",
      "position": [
        1056,
        448
      ],
      "typeVersion": 3.2
    },
    {
      "parameters": {
        "fields": {
          "values": [
            {
              "name": "HTML",
              "stringValue": "={{ ($json.HTML || \"HTML BODY CONTENT FOR THIS SEARCH RESULT IS NOT AVAILABLE\").replace(/<script[^>]*>([\\s\\S]*?)<\\/script>|<style[^>]*>([\\s\\S]*?)<\\/style>|<noscript[^>]*>([\\s\\S]*?)<\\/noscript>|<!--[\\s\\S]*?-->|<iframe[^>]*>([\\s\\S]*?)<\\/iframe>|<object[^>]*>([\\s\\S]*?)<\\/object>|<embed[^>]*>([\\s\\S]*?)<\\/embed>|<video[^>]*>([\\s\\S]*?)<\\/video>|<audio[^>]*>([\\s\\S]*?)<\\/audio>|<svg[^>]*>([\\s\\S]*?)<\\/svg>/ig, '')}}"
            }
          ]
        },
        "options": {}
      },
      "id": "90be31bf-407f-461a-9cce-c4ad9c6cb530",
      "name": "Remove extra tags",
      "type": "n8n-nodes-base.set",
      "position": [
        1248,
        944
      ],
      "typeVersion": 3.2
    },
    {
      "parameters": {
        "fields": {
          "values": [
            {
              "name": "HTML",
              "stringValue": "={{ $json.HTML.replace(/href\\s*=\\s*\"(.+?)\"/gi, 'href=\"NOURL\"').replace(/src\\s*=\\s*\"(.+?)\"/gi, 'src=\"NOIMG\"')}}"
            }
          ]
        },
        "options": {}
      },
      "id": "08cdebdb-aab1-4f8d-aa65-74de5e5271c5",
      "name": "Simplify output",
      "type": "n8n-nodes-base.set",
      "position": [
        1632,
        848
      ],
      "notesInFlow": true,
      "typeVersion": 3.2,
      "notes": "remove links and image URLs"
    },
    {
      "parameters": {
        "conditions": {
          "string": [
            {
              "value1": "={{ $('CONFIG').first()?.json?.query?.method }}",
              "operation": "contains",
              "value2": "simplif"
            }
          ]
        }
      },
      "id": "db9b67a7-05ee-40bc-9039-8451043f0755",
      "name": "Simplify?",
      "type": "n8n-nodes-base.if",
      "position": [
        1456,
        944
      ],
      "typeVersion": 1
    },
    {
      "parameters": {
        "fields": {
          "values": [
            {
              "name": "query",
              "stringValue": "={{   $json.output.match(/(https?:\\/\\/[^\\s]+|www\\.[^\\s]+)/)?.[0] || \"\" }}"
            }
          ]
        },
        "options": {}
      },
      "id": "fb72ab96-d99a-4476-aa63-6cc5f7083436",
      "name": "QUERY_PARAMS",
      "type": "n8n-nodes-base.set",
      "position": [
